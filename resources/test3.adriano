.data:
one_value_reg word 1;sdaasdasd
zero_value_reg word 0; sdadas asd
result_reg word 9; asdadds 
num_to_factorial word 10  
fact_param_reg word 10; asdasdda
;
;
;z
;s<
;asa,sda,sd
mult_param_1_reg word 19
;;

;;

;s
;a
;as,as,as
mult_param_2_reg word 18
curr_it_reg word 3

.code:
start:
	loadi 1 0 one_value_reg  0
	loadi 2 0 zero_value_reg  0 
	loadi 10 0 num_to_factorial 0
	loadi 9 0 0 0
	
fact_iteration:	
	add 9 2 19 0 0 0
	add 10 2 18 0 0 0
	sub 10 1 10 0 0 0
	cmp 10 0
	bz end
	bnz mult
mult: 
	add 9 19 9 0 0 0
	sub 18 1 18 0 0 0
	
	cmp 18 1
	bz fact_iteration
	bnz mult

end:
